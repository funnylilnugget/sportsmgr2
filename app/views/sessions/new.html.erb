<h1>Log In</h1>

<div>
  <div></div>

  <div>
    <div>
      <%= form_for(:session, url: login_path) do |f| %>

      <h3><%= f.label :email %></h3>
      <%= f.text_field :emai %><br>

      <h3><%= f.label :password %></h3>
      <%= f.password_field :password, placeholder: "Password" %>
      <br><br>
      <%= f.submit "Enter", class: "button" %>
      <br>
    </div>
  </div>
  <div></div>
</div>
<% end %>
